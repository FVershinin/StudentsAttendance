<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">
    <changeSet author="Vadim" id="modify teacher table">
        <addDefaultValue columnName="role"
                         defaultValue="USER"
                         tableName="teacher"/>
        <addDefaultValue columnName="password"
                defaultValue="pass"
                tableName="teacher"/>
    </changeSet>
    <changeSet author="Vadim" id="dropNotNullConstraint in teacher password">
        <dropNotNullConstraint columnName="password"
                               tableName="teacher"/>
    </changeSet>
    <changeSet id="modify password in teacher table" author="Vadim">
        <addDefaultValue columnName="password"
                         defaultValue="pass"
                         tableName="teacher"/>
    </changeSet>
</databaseChangeLog>